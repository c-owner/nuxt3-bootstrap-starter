{
    "root": true,
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "ignorePatterns": ["node_modules/*", "dist/*", "!.prettierrc.js", "!.*md"],
    "extends": ["plugin:@typescript-eslint/recommended", "plugin:nuxt/recommended", "plugin:prettier/recommended"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": ["error", {}]
    }
}
